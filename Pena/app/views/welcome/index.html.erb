<% if current_user %>
  <div class="profile">
    <div class-"user-info">
      <ul>
        <li><img src="<%= current_user.image_url %>"></li>
        <li><strong><%= current_user.name %></strong></li>
        <li><%= current_user.company %></li>
        <li><%= current_user.title %></li>
        <li><%= current_user.headline %></li>
        <li><%= current_user.email%></li>
      </ul>
    </div>
  </div>
  <div class='logout'>
    <%= link_to "Logout", logout_path, :method => :delete %><br>
    <%= link_to "Events", events_path %> </br>
    <%= link_to "Meetup Events", events_index2_path %>
  </div>
  <% else %>
    <%= render 'welcome' %>
  <% end %>
