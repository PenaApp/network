<%= form_for(@notes) do |f| %>
      <ul>
      <% @note.each do |note| %>
        <li><%= note  %></li>
      <% end %>
      </ul>
    </div>


  <div class="field">
    <%= f.label :note %><br>
    <%= f.text_area :body %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
  <% end %>
