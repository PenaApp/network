<%= form_for(@note) do |f| %>
      <ul>
      <% @note.each do |note| %>
        <li><%= note  %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :note %><br>
    <%= f.text_area :body %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
